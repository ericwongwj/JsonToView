<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tn_ma_l30000048.myjsontest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TestViewActivity" />
        <activity android:name=".activity.TestLayoutActivity" />
        <activity android:name=".activity.TestListActivity" />
        <activity android:name=".activity.TestJsonViewActivity" />
        <activity android:name=".activity.JsonToViewActivity" />
        <activity android:name=".activity.ShowJsonActivity" />
        <activity android:name=".activity.TestMyListActivity" />
        <activity android:name=".activity.TestGridActivity" />
        <activity android:name=".activity.TestRelativeActivity" />

        <receiver
            android:name=".receiver.MyReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="ELITOR_CLOCK" />
            </intent-filter>
        </receiver>
    </application>

</manifest>